<!-- displayData.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
</head>
<body>
    <h1><%= title %></h1>
    
    <% if (userData) { %>
        <p>Last Check:</p>
        <pre><%= JSON.stringify(userData, null, 2) %></pre>
    <% } else { %>
        <p>No user data available.</p>
    <% } %>
</body>
</html>
